{
  "name": "addTechBarista",
  "version": "1.0.0",
  "description": "addTechBarista",
  "main": "app.js",
  "scripts": {
    "migrations": "npx sequelize-cli db:migrate",
    "seeders": "npx sequelize-cli db:seed:all",
    "server": "nodemon app.js",
    "client": "npm run start --prefix ../client",
    "client:install": "cd ../client &&  npm install",
    "client:build": "npm run build --prefix ../client",
    "dev": "cross-env NODE_ENV=development concurrently  \"npm run server\" \"npm run client\" ",
    "start": "cross-env NODE_ENV=production node app.js"
  },
  "keywords": [
    "addTechBarista"
  ],
  "author": "A.Dolubaev",
  "license": "ISC",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.19.0",
    "config": "^3.3.6",
    "cookie-parser": "^1.4.5",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "express-handlebars": "^5.3.3",
    "express-validator": "^6.12.1",
    "fs": "^0.0.1-security",
    "glob": "^7.1.7",
    "jsonwebtoken": "^8.5.1",
    "moment": "^2.29.1",
    "multer": "^1.4.3",
    "pg": "^8.7.1",
    "pg-hstore": "^2.3.4",
    "sequelize": "^6.6.5",
    "shortid": "^2.2.16"
  },
  "devDependencies": {
    "concurrently": "^6.2.1",
    "cross-env": "^7.0.3",
    "nodemon": "^2.0.12",
    "sequelize-cli": "^6.2.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/testGitAza/addTechBarista.git"
  },
  "bugs": {
    "url": "https://github.com/testGitAza/addTechBarista/issues"
  },
  "homepage": "https://github.com/testGitAza/addTechBarista#readme"
}
